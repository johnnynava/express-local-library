extends layout

block content

  h1 #{title}: #{bookinstance.imprint}
  p= bookinstance.book.title

  p Do you really want to delete this Book Instance?

  form(method="POST")
    div.form-group
      input#bookid.form-control(type="hidden", name="bookinstanceid", value=bookinstance._id)

    button.btn.btn-primary(type="submit") Delete